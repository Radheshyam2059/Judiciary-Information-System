{
  "version": 2,
  "buildCommand": "echo 'Building...'",
  "builds": [
    {
      "src": "api/handler.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/handler.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/handler.js"
    }
  ]
}

